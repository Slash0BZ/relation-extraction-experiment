            <artifactId>cogcomp-datastore</artifactId>
            <version>1.9.7</version>
        </dependency>
        <dependency>
            <groupId>edu.illinois.cs.cogcomp</groupId>
            <artifactId>Illinois-Combined-Coref</artifactId>
            <version>1.0.4</version>
